.root {
  position: relative;
}

.blockContainer {
  float: left;
  width: 100%;
  height: 100px;
}

.blocks {
  float: left;
  width: 100%;
}

.block {
  color: white; 
  border: solid #eeeeee 1px;
  border-right: none;
  border-bottom: none;

  background: #0083d7;
  height: 25px;
  float: left;
  text-align: center;
}

.block:last-child {
  border-right: solid #eeeeee 1px;
}

.foo {
  top: 25px;
  width: 0.806452%;
  float: none;
  position: absolute;
}
